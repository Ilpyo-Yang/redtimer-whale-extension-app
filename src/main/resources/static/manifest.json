{
  "manifest_version": 2,
  "name": "로즈타이머",
  "version": "1.0.0",

  "description": "분단위 타이머로 시간관리하고 생산성을 높일 수 있습니다.",
  "icons": {
    "16": "./image/logo16.png",
    "48": "./image/logo48.png",
    "128": "./image/logo128.png"
  },
  "default_locale": "ko",

  "browser_action": {
    "default_popup": "../templates/index.html",
    "default_icon": {
      "16": "./image/logo16.png"
    }
  },
  "page_action": {},
  "sidebar_action": {},
  "permissions": [
    "<all_urls>"
  ]
}